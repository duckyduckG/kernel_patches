+
+	/*延时500ms*/
+	//mdelay(500);
+
+	if (!IS_ERR(d)) {
+		/*release sysfs node */
+		Nanosic_sysfs_release(d);
+	}
+
+	/* Step 1: Tear down our char device */
+	device_destroy(nanosic_cdev_class, nanosic_cdev_basedev + 0);
