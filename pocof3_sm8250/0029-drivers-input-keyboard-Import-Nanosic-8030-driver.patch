+	}
+
+	if (size <= 0) {
+		dbgprint(ERROR_LEVEL, "Invaild argment\n");
+		return -EINVAL;
+	}
+
+	if (IS_ERR(buf)) {
+		dbgprint(ERROR_LEVEL, "Invaild argment\n");
+		return -EINVAL;
+	}
+
+	rawdata_show("would not write i2c cmd", buf, size);
+	return -EINVAL;
