+        /* GPIO_24: FP_RESET_N */
+        msm_gpio_24: msm_gpio_24 {
+                mux {
+                         pins = "gpio24";
+                         function = "gpio";
+                };
+                config {
+                        pins = "gpio24";
+                        drive-strength = <2>;
+                        bias-disable;
+                        output-low;
+                };
+        };
+        /* GPIO_24 : FP_RESET_N, state device active*/
+        msm_gpio_24_output_high: msm_gpio_24_output_high {
+                mux {
+                        pins = "gpio24";
+                        function = "gpio";
+                };
+                config {
+                        pins = "gpio24";
+                        drive-strength = <2>;
+                        bias-disable;
+                        output-high;
+                };
+        };
+        /* GPIO_23 : FP_INT_N */
+        msm_gpio_23: msm_gpio_23 {
+                mux {
+                        pins = "gpio23";
+                        function = "gpio";
+                };
+                config {
+                        pins = "gpio23";
+                        drive-strength = <2>;
+                        bias-pull-down;
+                };
+        };
+
+};
+
+&soc {
+
+	testing_mode {
+		compatible = "xiaomi,testing-mode";
+		status-gpio = <&tlmm 70 0x00>;
+		debounce-time = <30>;
+		pinctrl-names = "default";
+		pinctrl-0 = <&test_mode_irq>;
+	};
+
+};
+
+&ts_active {
+	mux {
+		pins = "gpio33", "gpio39";
+		function = "gpio";
+	};
+
+	config {
+		pins = "gpio33", "gpio39";
+		drive-strength = <8>;
+		bias-pull-up;
+	};
+};
+
+&ts_int_suspend {
+	mux {
+		pins = "gpio39";
+		function = "gpio";
+	};
+
+	config {
+		pins = "gpio39";
+		drive-strength = <2>;
+		bias-pull-down;
+	};
+};
+
+&ts_reset_suspend {
+	mux {
+		pins = "gpio33";
+		function = "gpio";
+	};
+
+	config {
+		pins = "gpio33";
+		drive-strength = <2>;
+		bias-pull-down;
+	};
+};
+
+&pmx_ts_release {
+	mux {
+		pins = "gpio33", "gpio39";
+		function = "gpio";
+	};
+
+	config {
+		pins = "gpio33", "gpio39";
+		drive-strength = <2>;
+		bias-disable;
+	};
+};
+&tlmm {
+	ts_mux {
+		ts_cs_active: ts_cs_active {
+			mux {
+				pins = "gpio11";
+				function = "qup4";
+			};
+
+			config {
+				pins = "gpio11";
+				drive-strength = <16>;
+				bias-pull-up;
+			};
+		};
+
+		ts_cs_suspend: ts_cs_suspend {
+			mux {
+				pins = "gpio11";
+				function = "gpio";
+			};
+
+			config {
+				pins = "gpio11";
+				drive-strength = <16>;
+				bias-pull-up;
+			};
+		};
+	};
+};
+
+
+&qupv3_se4_spi_active {
+	mux {
+		pins = "gpio8", "gpio9", "gpio10";
+		function = "qup4";
+	};
+
+	config {
+		pins = "gpio8", "gpio9", "gpio10";
+		drive-strength = <6>;
+		bias-pull-down;
+	};
+};
+
+&qupv3_se4_spi_sleep {
+	mux {
+		pins = "gpio8", "gpio9", "gpio10";
+		function = "gpio";
+	};
+
+	config {
+		pins = "gpio8", "gpio9", "gpio10";
+		drive-strength = <6>;
+		bias-pull-down;
+	};
+};
+
+
+&qupv3_se4_spi {
+	status = "ok";
+	pinctrl-0 = <&qupv3_se4_spi_active &ts_cs_active>;
+	pinctrl-1 = <&qupv3_se4_spi_sleep &ts_cs_suspend>;
+	focaltech@0 {
+		status = "ok";
+		compatible = "focaltech,fts";
+		reg = <0x0>;
+		spi-max-frequency = <6000000>;
+		interrupt-parent = <&tlmm>;
+		interrupts = <39 0x2008>;
+		pinctrl-names = "pmx_ts_active", "pmx_ts_suspend", "pmx_ts_release";
