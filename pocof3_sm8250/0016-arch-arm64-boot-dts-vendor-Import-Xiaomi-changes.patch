+				/* GPIO 82 is used for wireless charge enable bq by output vol to bq vac */
+				pins = "gpio82";
+				function = "gpio";
+			};
+
+			config {
+				pins = "gpio82";
+				bias-disable; /* No PULL */
+				drive-strength = <16>; /* 16 MA */
+				output-low;
+			};
