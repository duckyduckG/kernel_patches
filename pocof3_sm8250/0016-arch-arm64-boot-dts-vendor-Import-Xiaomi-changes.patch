@@ -0,0 +1,538 @@
+/*for L11R pinctrl */
+
+&sde_dsi_active {
+	mux {
+		pins = "gpio12", "gpio50";
+		function = "gpio";
+	};
+
+	config {
+		pins = "gpio12", "gpio50";
+		drive-strength = <8>;	/* 8 mA */
+		bias-disable = <0>;   /* no pull */
+	};
+};
+
+&sde_dsi_suspend {
+	mux {
+		pins = "gpio12", "gpio50";
+		function = "gpio";
+	};
+
+	config {
+		pins = "gpio12", "gpio50";
+		drive-strength = <2>;	/* 2 mA */
+		bias-pull-down; 		/* PULL DOWN */
+	};
+};
+
+&nfc_enable_active {
+	/* active state */
+	mux {
+		/* 83: Enable 110: Firmware */
+		pins = "gpio83", "gpio110";
+		function = "gpio";
+	};
+
+	config {
+		pins = "gpio83", "gpio110";
+		drive-strength = <2>; /* 2 MA */
+		bias-pull-up;
+	};
+};
+
+&nfc_enable_suspend {
+	/* sleep state */
+	mux {
+		/* 83: Enable 110: Firmware */
+		pins = "gpio83", "gpio110";
+		function = "gpio";
+	};
+
+	config {
+		pins = "gpio83", "gpio110";
+		drive-strength = <2>; /* 2 MA */
+		bias-disable;
+	};
+};
+
+
+&tlmm {
+         md_mode_ctrl_irq {
+		ant2_ctrl_irq: ant2_ctrl_irq {
+			mux {
+				pins = "gpio14";
+				function = "gpio";
+			};
+
+			config {
+				pins = "gpio14";
+				drive-strength = <2>;
+				bias-disable;
+				input-enable;
+			};
+		};
+
+		ant5_ctrl_irq: ant5_ctrl_irq {
+			mux {
+				pins = "gpio15";
+				function = "gpio";
+			};
+
+			config {
+				pins = "gpio15";
+				drive-strength = <2>;
+				bias-disable;
+				input-enable;
+			};
+                };
+	};
+
+	onewire_gpio_active: onewire_gpio_active {
